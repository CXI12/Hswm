{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ],
  "regions": [
    "hkg1",
    "hnd1"
  ],
  "functions": {
    "**": {
      "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
    }
  }
}